{"ast":null,"code":"import { useRoute } from \"vue-router\";\nimport { onMounted, reactive } from \"vue\";\nimport { getMusicItemList } from \"@/request/api/item.js\";\nimport ItemMusicTop from \"@/components/item/ItemMusicTop.vue\";\nimport ItemMusicList from \"@/components/item/ItemMusicList.vue\";\nimport { getItemList } from \"@/request/api/item.js\";\nexport default {\n  setup() {\n    const state = reactive({\n      playlist: {}\n    });\n    onMounted(async () => {\n      let id = useRoute().query.id; // console.log(useRoute());\n      //   console.log(id);\n      // 获取歌单详情页\n\n      let res = await getMusicItemList(id); //   console.log(res);\n\n      state.playlist = res.data.playlist; // 获取歌单的歌曲\n      // 防止页面刷新 造成数据丢失，将数据保存到 sessionStorage\n\n      sessionStorage.setItem(\"itemDetail\", JSON.stringify(state));\n    });\n    return {\n      state\n    };\n  },\n\n  components: {\n    ItemMusicTop,\n    ItemMusicList\n  }\n};","map":{"version":3,"mappings":"AAQA,SAASA,QAAT,QAAyB,YAAzB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,KAApC;AACA,SAASC,gBAAT,QAAiC,uBAAjC;AACA,OAAOC,YAAP,MAAyB,oCAAzB;AACA,OAAOC,aAAP,MAA0B,qCAA1B;AACA,SAASC,WAAT,QAA4B,uBAA5B;AAEA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,QAAQ,EAAE;IADW,CAAD,CAAtB;IAGAR,SAAS,CAAC,YAAY;MACpB,IAAIS,EAAC,GAAIV,QAAQ,GAAGW,KAAX,CAAiBD,EAA1B,CADoB,CAEpB;MACA;MAEA;;MACA,IAAIE,GAAE,GAAI,MAAMT,gBAAgB,CAACO,EAAD,CAAhC,CANoB,CAOpB;;MACAF,KAAK,CAACC,QAAN,GAAiBG,GAAG,CAACC,IAAJ,CAASJ,QAA1B,CARoB,CAUpB;MAEA;;MACAK,cAAc,CAACC,OAAf,CAAuB,YAAvB,EAAqCC,IAAI,CAACC,SAAL,CAAeT,KAAf,CAArC;IACD,CAdQ,CAAT;IAeA,OAAO;MAAEA;IAAF,CAAP;EACD,CArBY;;EAsBbU,UAAU,EAAE;IACVd,YADU;IAEVC;EAFU;AAtBC,CAAf","names":["useRoute","onMounted","reactive","getMusicItemList","ItemMusicTop","ItemMusicList","getItemList","setup","state","playlist","id","query","res","data","sessionStorage","setItem","JSON","stringify","components"],"sourceRoot":"","sources":["E:\\Project_2022\\web\\Vue\\练习\\NetMusic\\NetMusic\\netmusic\\src\\views\\ItemMusic.vue"],"sourcesContent":["<template>\r\n  <div class=\"itemMusic\">\r\n    <ItemMusicTop :playlist=\"state.playlist\" />\r\n    <ItemMusicList />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRoute } from \"vue-router\";\r\nimport { onMounted, reactive } from \"vue\";\r\nimport { getMusicItemList } from \"@/request/api/item.js\";\r\nimport ItemMusicTop from \"@/components/item/ItemMusicTop.vue\";\r\nimport ItemMusicList from \"@/components/item/ItemMusicList.vue\";\r\nimport { getItemList } from \"@/request/api/item.js\"\r\n\r\nexport default {\r\n  setup() {\r\n    const state = reactive({\r\n      playlist: {},\r\n    });\r\n    onMounted(async () => {\r\n      let id = useRoute().query.id;\r\n      // console.log(useRoute());\r\n      //   console.log(id);\r\n\r\n      // 获取歌单详情页\r\n      let res = await getMusicItemList(id);\r\n      //   console.log(res);\r\n      state.playlist = res.data.playlist;\r\n\r\n      // 获取歌单的歌曲\r\n    \r\n      // 防止页面刷新 造成数据丢失，将数据保存到 sessionStorage\r\n      sessionStorage.setItem(\"itemDetail\", JSON.stringify(state));\r\n    });\r\n    return { state };\r\n  },\r\n  components: {\r\n    ItemMusicTop,\r\n    ItemMusicList,\r\n  },\r\n};\r\n</script>"]},"metadata":{},"sourceType":"module"}