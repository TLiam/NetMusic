{"ast":null,"code":"import { useRoute } from \"vue-router\";\nimport { onMounted, reactive } from \"vue\";\nimport { getMusicItemList } from \"@/request/api/item.js\";\nimport ItemMusicTop from \"@/components/item/ItemMusicTop.vue\";\nimport ItemMusicList from \"@/components/item/ItemMusicList.vue\";\nimport { getItemList } from \"@/request/api/item.js\";\nimport { stat } from \"fs\";\nexport default {\n  setup() {\n    const state = reactive({\n      playlist: {},\n      // 歌单详情页的数据\n      itemlist: [] // 歌单的歌曲数据\n\n    });\n    onMounted(async () => {\n      let id = useRoute().query.id; // console.log(useRoute());\n      //   console.log(id);\n      // 获取歌单详情页\n\n      let res = await getMusicItemList(id); //   console.log(res);\n\n      state.playlist = res.data.playlist; // 获取歌单的歌曲\n\n      let result = await getItemList(id);\n      console.log(result);\n      state.itemlist = result.songs; // 防止页面刷新 造成数据丢失，将数据保存到 sessionStorage\n\n      sessionStorage.setItem(\"itemDetail\", JSON.stringify(state));\n    });\n    return {\n      state\n    };\n  },\n\n  components: {\n    ItemMusicTop,\n    ItemMusicList\n  }\n};","map":{"version":3,"mappings":"AAQA,SAASA,QAAT,QAAyB,YAAzB;AACA,SAASC,SAAT,EAAoBC,QAApB,QAAoC,KAApC;AACA,SAASC,gBAAT,QAAiC,uBAAjC;AACA,OAAOC,YAAP,MAAyB,oCAAzB;AACA,OAAOC,aAAP,MAA0B,qCAA1B;AACA,SAASC,WAAT,QAA4B,uBAA5B;AACA,SAASC,IAAT,QAAqB,IAArB;AAEA,eAAe;EACbC,KAAK,GAAG;IACN,MAAMC,KAAI,GAAIP,QAAQ,CAAC;MACrBQ,QAAQ,EAAE,EADW;MACP;MACdC,QAAQ,EAAE,EAFW,CAEP;;IAFO,CAAD,CAAtB;IAIAV,SAAS,CAAC,YAAY;MACpB,IAAIW,EAAC,GAAIZ,QAAQ,GAAGa,KAAX,CAAiBD,EAA1B,CADoB,CAEpB;MACA;MAEA;;MACA,IAAIE,GAAE,GAAI,MAAMX,gBAAgB,CAACS,EAAD,CAAhC,CANoB,CAOpB;;MACAH,KAAK,CAACC,QAAN,GAAiBI,GAAG,CAACC,IAAJ,CAASL,QAA1B,CARoB,CAUpB;;MACA,IAAIM,MAAK,GAAI,MAAMV,WAAW,CAACM,EAAD,CAA9B;MACAK,OAAO,CAACC,GAAR,CAAYF,MAAZ;MACAP,KAAK,CAACE,QAAN,GAAiBK,MAAM,CAACG,KAAxB,CAboB,CAcpB;;MACAC,cAAc,CAACC,OAAf,CAAuB,YAAvB,EAAqCC,IAAI,CAACC,SAAL,CAAed,KAAf,CAArC;IACD,CAhBQ,CAAT;IAiBA,OAAO;MAAEA;IAAF,CAAP;EA