{"ast":null,"code":"import { Vue3Marquee } from \"vue3-marquee\";\nimport \"vue3-marquee/dist/style.css\";\nimport { mapMutations, mapState } from \"vuex\";\nexport default {\n  data() {\n    return {\n      isLyricShow: false\n    };\n  },\n\n  computed: { ...mapState([\"lyricList\", \"currentTime\", \"playListIndex\", \"playList\"]),\n    lyric: function () {\n      let arr;\n\n      if (this.lyricList.lyric) {\n        arr = this.lyricList.lyric.split(/[(\\r\\n)\\r\\n]+/).map((item, i) => {\n          let min = item.slice(1, 3);\n          let sec = item.slice(4, 6);\n          let mill = item.slice(7, 10);\n          let lyc = item.slice(11);\n          let time = parseInt(min) * 60 * 1000 + parseInt(sec) * 1000 + parseInt(mill);\n\n          if (isNaN(Number(mill))) {\n            mill = item.slice(7, 9);\n            lyc = item.slice(10);\n            time = parseInt(min) * 60 * 1000 + parseInt(sec) * 1000 + parseInt(mill);\n          } //   console.log(min, sec, mill, lyc);\n\n\n          return {\n            min,\n            sec,\n            mill,\n            lyc,\n            time\n          };\n        });\n        arr.forEach((item, i) => {\n          if (i === arr.length - 1 && isNaN()) {\n            item.pre = 0;\n          } else {\n            item.pre = arr[i + 1].time;\n          }\n        });\n      }\n\n      console.log(arr);\n      return arr;\n    }\n  },\n\n  mounted() {// console.log(this.musicList);\n    // console.log(this.lyricList.lyric);\n  },\n\n  watch: {\n    currentTime: function () {\n      let p = document.querySelector(\"p.active\"); //   console.log([p]);\n\n      if (p.offsetTop > 300) {\n        this.$refs.musicLyric.scrollTop = p.offsetTop - 300;\n      } //   console.log([this.$ref.musicLyric]);\n\n    }\n  },\n  props: [\"musicList\", \"isBtnShow\", \"play\"],\n  methods: {\n    backHome: function () {\n      this.isLyricShow = false;\n      this.updateDetailShow();\n    },\n    goPlay: function (num) {\n      let index = this.playListIndex + num;\n\n      if (index < 0) {\n        index = this.playList.length - 1;\n      } else if (index == this.playList.length) {\n        index = 0;\n      }\n\n      this.updatePlayListIndex(index);\n    },\n    ...mapMutations([\"updateDetailShow\", \"updatePlayListIndex\"])\n  },\n  components: {\n    Vue3Marquee\n  }\n};","map":{"version":3,"mappings":"AAuHA,SAASA,WAAT,QAA4B,cAA5B;AACA,OAAO,6BAAP;AACA,SAASC,YAAT,EAAuBC,QAAvB,QAAuC,MAAvC;AACA,eAAe;EACbC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE;IADR,CAAP;EAGD,CALY;;EAMbC,QAAQ,EAAE,EACR,GAAGH,QAAQ,CAAC,CAAC,WAAD,EAAc,aAAd,EAA4B,eAA5B,EAA6C,UAA7C,CAAD,CADH;IAERI,KAAK,EAAE,YAAY;MACjB,IAAIC,GAAJ;;MACA,IAAI,KAAKC,SAAL,CAAeF,KAAnB,EAA0B;QACxBC,GAAE,GAAI,KAAKC,SAAL,CAAeF,KAAf,CAAqBG,KAArB,CAA2B,eAA3B,EAA4CC,GAA5C,CAAgD,CAACC,IAAD,EAAOC,CAAP,KAAa;UACjE,IAAIC,GAAE,GAAIF,IAAI,CAACG,KAAL,CAAW,CAAX,EAAc,CAAd,CAAV;UACA,IAAIC,GAAE,GAAIJ,IAAI,CAACG,KAAL,CAAW,CAAX,EAAc,CAAd,CAAV;UACA,IAAIE,IAAG,GAAIL,IAAI,CAACG,KAAL,CAAW,CAAX,EAAc,EAAd,CAAX;UACA,IAAIG,GAAE,GAAIN,IAAI,CAACG,KAAL,CAAW,EAAX,CAAV;UACA,IAAII,IAAG,GACLC,QAAQ,CAACN,GAAD,CAAR,GAAgB,EAAhB,GAAqB,IAArB,GAA4BM,QAAQ,CAACJ,GAAD,CAAR,GAAgB,IAA5C,GAAmDI,QAAQ,CAACH,IAAD,CAD7D;;UAEA,IAAII,KAAK,CAACC,MAAM,CAACL,IAAD,CAAP,CAAT,EAAyB;YACvBA,IAAG,GAAIL,IAAI,CAACG,KAAL,CAAW,CAAX,EAAc,CAAd,CAAP;YACAG,GAAE,GAAIN,IAAI,CAACG,KAAL,CAAW,EAAX,CAAN;YACAI,IAAG,GACDC,QAAQ,CAACN,GAAD,CAAR,GAAgB,EAAhB,GAAqB,IAArB,GAA4BM,QAAQ,CAACJ,GAAD,CAAR,GAAgB,IAA5C,GAAmDI,QAAQ,CAACH,IAAD,CAD7D;UAEF,CAZiE,CAajE;;;UACA,OAAO;YAAEH,GAAF;YAAOE,GAAP;YAAYC,IAAZ;YAAkBC,GAAlB;YAAuBC;UAAvB,CAAP;QACD,CAfK,CAAN;QAgBAX,GAAG,CAACe,OAAJ,CAAY,CAACX,IAAD,EAAOC,CAAP,KAAa;UACvB,IAAIA,MAAML,GAAG,CAACgB,MAAJ,GAAa,CAAnB,IAAwBH,KAAK,EAAjC,EAAqC;YACnCT,IAAI,CAACa,GAAL,GAAW,CAAX;UACF,CAFA,MAEO;YACLb,IAAI,CAACa,GAAL,GAAWjB,GAAG,CAACK,IAAI,CAAL,CAAH,CAAWM,IAAtB;UACF;QACD,CAND;MAOF;;MACAO,OAAO,CAACC,GAAR,CAAYnB,GAAZ;MACA,OAAOA,GAAP;IACD;EA/BO,CANG;;EAuCboB,OAAO,GAAG,CACR;IACA;EACD,CA1CY;;EA2CbC,KAAK,EAAE;IACLC,WAAW,EAAE,Y